DEFAULT gluster-client
IMPLICIT 0
PROMPT 1
NOESCAPE 1
# 15 seconds timeout
TIMEOUT 150
ALLOWOPTIONS 1
LABEL gluster-client
    KERNEL linux.slave
    APPEND initrd=initrd.slave.gz gluster_bootp=yes ramdisk_size=65536

LABEL safe
    KERNEL linux.slave
    APPEND initrd=initrd.slave.gz gluster_bootp=yes ramdisk_size=65536 acpi=off noapic

